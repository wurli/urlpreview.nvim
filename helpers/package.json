{
    "name": "extract-metadata",
    "version": "1.0.0",
    "description": "Extract title and description from a URL",
    "main": "get_url_info.js",
    "bin": {
        "get-url-info": "./get_url_info.js"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.12"
    },
    "devDependencies": {
        "pkg": "^5.8.1"
    },
    "scripts": {
        "start": "node get_url_info.js",
        "build": "pkg ."
    },
    "pkg": {
        "targets": [
            "node16-linux-x64",
            "node16-macos-x64",
            "node16-win-x64"
        ],
        "outputPath": "dist"
    }
}
